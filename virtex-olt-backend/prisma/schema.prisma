generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model OltData {
  id    Int     @id @default(autoincrement())
  slot  String
  port  String
  ont   String
  sn    String
  state String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("olt_data")
}
